# Frontend Environment Variables

# Contract Address (after deployment)
NEXT_PUBLIC_CONTRACT_ADDRESS=0xYourContractAddressHere

# FHE Contract Address (for privacy-preserving version)
NEXT_PUBLIC_FHE_CONTRACT_ADDRESS=0xYourFHEContractAddressHere

# Pinata IPFS Configuration
NEXT_PUBLIC_PINATA_JWT=your_pinata_jwt_here
NEXT_PUBLIC_PINATA_GATEWAY=https://your-gateway.mypinata.cloud

# WalletConnect Project ID (get from reown.com)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Infura API Key (for Sepolia FHEVM)
NEXT_PUBLIC_INFURA_API_KEY=your_infura_api_key_here

# Alchemy API Key (alternative to Infura)
NEXT_PUBLIC_ALCHEMY_API_KEY=your_alchemy_api_key_here
